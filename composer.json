{
    "name": "ns/color-admin-bundle",
    "description": "Template bundle for Color Admin",
    "keywords": ["template"],
    "homepage": "https://github.com/NobletSolutions/ColorAdminBundle",
    "authors": [
        {
            "name": "Mark Woynarski",
            "email": "mark.woynarski@nobletdesign.com"
        },
        {
            "name": "Nathanael David Noblet",
            "email": "nathanael@noblet.ca"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-json": "*",
        "symfony/framework-bundle": "^3.0||^4.0||^5.0||^6.0",
        "symfony/form":             "^3.0||^4.0||^5.0||^6.0",
        "symfony/event-dispatcher": "^3.0||^4.0||^5.0||^6.0",
        "symfony/routing":          "^3.0||^4.0||^5.0||^6.0",
        "twig/twig":                "^2.0|^3.0",
        "knplabs/knp-menu-bundle":  "^2.1|^3.0",
        "ns/flash-bundle":          "^1.0|^2.0|^3.0",
        "doctrine/orm":             "^2.5|^2.6",
        "symfony/doctrine-bridge": "^6.2"
    },
    "suggest": {
        "knplabs/knp-paginator-bundle": "^2.5"
    },
    "autoload": {
        "psr-4": { "NS\\ColorAdminBundle\\": "src" }
    },
    "autoload-dev": {
        "psr-4": { "Tests\\NS\\ColorAdminBundle\\": "tests" }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0|^7.0|^9.0",
        "liip/rmt": "^1.6"
    }
}
